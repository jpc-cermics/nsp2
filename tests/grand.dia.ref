-nsp->PS=(1:N)'/N;
-nsp->//plot2d(RdevS,PS);
-nsp->// theorical result 
-nsp->[P]=cdfbet("PQ",RdevS,1-RdevS,A*ones(size(RdevS)),B*ones(size(RdevS)));
-nsp->//plot2d(RdevS,P,style=2);
-nsp->if norm(P-PS) > prec then pause,end
