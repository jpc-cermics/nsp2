;; -*- scheme -*-

(define-objectref Polyline
  (in-module "Polyline")
  (parent "Graphic")
  (c-name "NspPolyline")
  (fields
    '("NspMatrix*" "x")
    '("NspMatrix*" "y")
    '("gboolean" "close" "std" "FALSE")
    '("int" "color" "std" "-1")
    '("int" "mark" "std" "-1")
    '("int" "mark_size" "std" "-1")
    '("int" "mark_color" "std" "-1")
    '("int" "fill_color" "std" "-1")
    '("int" "thickness")
    )
  (gtype-id "Polyline")
)
