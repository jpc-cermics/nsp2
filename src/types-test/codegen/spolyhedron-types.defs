;; -*- scheme -*-

(define-objectref SPolyhedron
  (in-module "SPolyhedron")
  (parent "Graphic")
  (c-name "SPolyhedron")
  (fields
    '("NspMatrix*" "x")
    '("NspMatrix*" "y")
    '("NspMatrix*" "z")
    '("gboolean" "mesh" "std" "TRUE")
    '("int" "mesh_color" "std" "-1")
    '("int" "face_color" "std" "-1")
    )
  (gtype-id "SPolyhedron")
)




