;; -*- scheme -*-

(define-objectref SPolyhedron
  (in-module "SPolyhedron")
  (parent "Graphic")
  (c-name "SPolyhedron")
  (fields
   '("NspMatrix*" "Mcoord")
   '("NspMatrix*" "Mface")
   '("NspMatrix*" "Mval")
   '("double" "vmin")
   '("double" "vmax")
   '("double" "colmin")
   '("double" "colmax")
   '("double" "coloutmin")
   '("double" "coloutmax")
   '("gboolean" "mesh" "std" "TRUE")
   '("int" "back_color" )
   ;;  extra hidden fields 
   '("void*" "Mcoord_l"  "hidden" "NULL" )
   '("int*" "pos" "hidden" )
   '("int*" "fill" "hidden" )
   '("double*" "vlevel" "hidden" )
    )
  (gtype-id "SPolyhedron")
)




