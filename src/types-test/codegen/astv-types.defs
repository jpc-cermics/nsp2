;; -*- scheme -*-
;;
;; variables to be used in conjunction with ast

(define-object Astv
  (in-module "Astv")
  (parent "Object")
  (c-name "NspAstv")
  (fields
   '("gboolean" "hv" "hidden" "FALSE"); /* number of rows */
   '("int" "rows" "hidden"); /* number of rows */
   '("int" "columns" "hidden"); /* number of columns */
   '("char*" "stype" "hidden"); /* type as string */
   '("NspObject*" "value" "hidden" "NULL" ); /* value as object or NULL  */
   )
  (gtype-id "Astv")
)









