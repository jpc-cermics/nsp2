;; -*- scheme -*-

(define-objectref Fec
  (in-module "Fec")
  (parent "Graphic")
  (c-name "NspFec")
  (fields
    '("NspMatrix*" "x") ;; 
    '("NspMatrix*" "y") ;; 
    '("NspMatrix*" "triangles") ;; 
    '("NspMatrix*" "func") ;; 
    '("NspMatrix*" "colminmax" );; 
    '("NspMatrix*" "zminmax") ;; 
    '("gboolean"   "mesh")   ;; draw the triangles 
    '("gboolean"   "paint" "std" "TRUE")  ;; fill the triangles 
    '("NspMatrix*" "colout")
    )
  (gtype-id "Fec")
)

