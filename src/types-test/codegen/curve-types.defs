;; -*- scheme -*-

(define-objectref Curve
  (in-module "Curve")
  (parent "Graphic")
  (c-name "Curve")
  (fields
    '("int" "color"); curve color 
    '("int" "mark") ; mark to be used 
    '("double" "width"); line width 
    '("int" "style"); line style 
    '("int" "mode"); mode: std, step, stem, arrow.
    '("NspMatrix*" "Pts")
    )
  (gtype-id "Curve")
)




