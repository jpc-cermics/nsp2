;; -*- scheme -*-
;;
;; variables to be used in conjunction with ast

(define-object Bvar
  (in-module "Bvar")
  (parent "Object")
  (c-name "NspBvar")
  (fields
   '("gboolean" "sym" "hidden" "FALSE"); /* symbolic ? */
   '("NspObject*" "value" "hidden" "NULL" ); /* value as object */
   '("char*" "varname" "hidden" ); /* if a value is given here we will have memory leaks: XXXXX */
   )
  (gtype-id "Bvar")
)









