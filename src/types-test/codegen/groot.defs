;; -*- scheme -*-

;; Graphic root object 
;; used to contain a list of figure 

(define-objectref GRoot 
  (in-module "GRoot")
  (parent "Object")
  (c-name "NspGRoot")
  (fields
   '("NspList*" "figures")
   )
  (gtype-id "GRoot")
)

