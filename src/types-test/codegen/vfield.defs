;; -*- scheme -*-

(define-objectref VField
  (in-module "VField")
  (parent "Graphic")
  (c-name "NspVField")
  (fields
    '("NspMatrix*" "fx") ;; Matrix 
    '("NspMatrix*" "fy") ;; Matrix 
    '("NspMatrix*" "x")  ;; vector 
    '("NspMatrix*" "y")  ;; vector 
    '("gboolean" "colored")  ;; colored arrows 
    )
  (gtype-id "VField")
)

